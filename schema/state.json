{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "State",
  "type": "object",
  "required": [
    "accepted_token",
    "admin",
    "offered_token",
    "percent_of_funding_collected",
    "sale_end_time",
    "viewing_key"
  ],
  "properties": {
    "accepted_token": {
      "$ref": "#/definitions/SecretContract"
    },
    "admin": {
      "$ref": "#/definitions/HumanAddr"
    },
    "offered_token": {
      "$ref": "#/definitions/SecretContract"
    },
    "percent_of_funding_collected": {
      "type": "integer",
      "format": "uint8",
      "minimum": 0.0
    },
    "sale_end_time": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "viewing_key": {
      "type": "string"
    }
  },
  "definitions": {
    "HumanAddr": {
      "type": "string"
    },
    "SecretContract": {
      "type": "object",
      "required": [
        "address",
        "contract_hash"
      ],
      "properties": {
        "address": {
          "$ref": "#/definitions/HumanAddr"
        },
        "contract_hash": {
          "type": "string"
        }
      }
    }
  }
}
